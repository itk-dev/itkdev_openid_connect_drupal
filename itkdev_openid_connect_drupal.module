<?php

use Drupal\itkdev_openid_connect_drupal\Helper\FormHelper;
use Drupal\Core\Form\FormStateInterface;

/**
 * Implements hook_form_alter().
 */
function itkdev_openid_connect_drupal_form_alter(array &$form, FormStateInterface $form_state, string $form_id) {
  $helper = \Drupal::service(FormHelper::class);
  $helper->alterForm($form, $form_state, $form_id);
}
